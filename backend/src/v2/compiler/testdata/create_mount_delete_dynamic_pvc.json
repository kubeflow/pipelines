            "access_modes": {
              "parameterType": "LIST"
            },
            "annotations": {
              "isOptional": true,
              "parameterType": "STRUCT"
            },
            "pvc_name": {
              "isOptional": true,
              "parameterType": "STRING"
            },
            "pvc_name_suffix": {
              "isOptional": true,
              "parameterType": "STRING"
            },
            "size": {
              "parameterType": "STRING"
            },
            "storage_class_name": {
              "defaultValue": "",
              "isOptional": true,
              "parameterType": "STRING"
            },
            "volume_name": {
              "isOptional": true,
              "parameterType": "STRING"
            }
          }
        },
        "outputDefinitions": {
          "parameters": {
            "name": {
              "parameterType": "STRING"
            }
          }
