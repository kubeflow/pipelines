{
  "pipelineSpec": {
    "pipelineInfo": {
      "name": "dummy-custom-job-pipeline"
    },
    "components": {
      "comp-test-custom-job": {
        "executorLabel": "exec-test-custom-job",
        "inputDefinitions": {
          "artifacts": {
            "input_text": {
              "artifactType": {
                "instanceSchema": "title: kfp.Dataset\ntype: object\nproperties:\n  payload_format:\n    type: string\n  container_format:\n    type: string\n"
              }
            }
          }
        }
      },
      "comp-write-to-gcs": {
        "outputDefinitions": {
          "artifacts": {
            "output_text": {
              "artifactType": {
                "instanceSchema": "title: kfp.Dataset\ntype: object\nproperties:\n  payload_format:\n    type: string\n  container_format:\n    type: string\n"
              }
            }
          }
        },
        "inputDefinitions": {
          "parameters": {
            "text": {
              "type": "STRING"
            }
          }
        },
        "executorLabel": "exec-write-to-gcs"
      }
    },
    "sdkVersion": "kfp-1.4.0",
    "schemaVersion": "2.0.0",
    "deploymentSpec": {
      "executors": {
        "exec-write-to-gcs": {
          "container": {
            "command": [
              "sh",
              "-c",
              "set -e -x\necho \"$0\" | gsutil cp - \"$1\"\n",
              "{{$.inputs.parameters['text']}}",
              "{{$.outputs.artifacts['output_text'].uri}}"
            ],
            "image": "google/cloud-sdk:slim"
          }
        },
        "exec-test-custom-job": {
          "customJob": {
            "customJob": {
              "jobSpec": {
                "workerPoolSpecs": [
                  {
                    "containerSpec": {
                      "command": [
                        "sh",
                        "-c",
                        "set -e -x\ngsutil cat \"$0\"\n",
                        "{{$.inputs.artifacts['input_text'].uri}}"
                      ],
                      "imageUri": "google/cloud-sdk:slim"
                    },
                    "replicaCount": "1",
                    "machineSpec": {
                      "machineType": "n1-standard-4"
                    }
                  }
                ]
              },
              "name": "test-custom-job"
            }
          }
        }
      }
    },
    "root": {
      "dag": {
        "tasks": {
          "task-test-custom-job": {
            "inputs": {
              "artifacts": {
                "input_text": {
                  "taskOutputArtifact": {
                    "outputArtifactKey": "output_text",
                    "producerTask": "task-write-to-gcs"
                  }
                }
              }
            },
            "taskInfo": {
              "name": "task-test-custom-job"
            },
            "componentRef": {
              "name": "comp-test-custom-job"
            }
          },
          "task-write-to-gcs": {
            "componentRef": {
              "name": "comp-write-to-gcs"
            },
            "taskInfo": {
              "name": "task-write-to-gcs"
            },
            "inputs": {
              "parameters": {
                "text": {
                  "runtimeValue": {
                    "constantValue": {
                      "stringValue": "hello world"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "runtimeConfig": {
    "gcsOutputDirectory": "gs://my-bucket/my-output-dir"
  }
}
