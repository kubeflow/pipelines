          },
          "task-print-op": {
            "inputs": {
              "parameters": {
                "pipelineparam--name": {
                  "componentInputParameter": "name"
                },
                "name": {
                  "runtimeValue": {
                    "constantValue": {
                      "stringValue": "Hello {{$.inputs.parameters['pipelineparam--name']}}"
                    }
                  }
                }
              }
            },
            "taskInfo": {
              "name": "task-print-op"
