{
  "pipelineSpec": {
    "deploymentSpec": {
      "executors": {
        "exec-ingestion": {
          "container": {
            "args": [
              "--input-location",
              "{{$.inputs.parameters['input_location']}}",
              "--output-examples",
              "{{$.outputs.artifacts['examples'].uri}}"
            ],
            "image": "gcr.io/my-project/my-ingestor"
          }
        },
        "exec-trainer": {
          "container": {
            "resources": {
              "accelerator": {
                "count": "1",
                "type": "TPU_V3"
              },
              "cpuLimit": 4.0,
              "memoryLimit": 15.032385536
            },
            "image": "gcr.io/my-project/my-fancy-trainer",
            "args": [
              "--input-examples",
              "{{$.inputs.artifacts['examples'].uri}}",
              "--optimizer",
              "{{$.inputs.parameters['optimizer']}}",
              "--n_epochs",
              "{{$.inputs.parameters['n_epochs']}}",
              "--output-model",
              "{{$.outputs.artifacts['model'].uri}}"
            ]
          }
        }
      }
    },
    "sdkVersion": "kfp-1.3.0",
    "pipelineInfo": {
      "name": "two-step-pipeline-with-resource-spec"
    },
    "schemaVersion": "2.0.0",
    "components": {
      "comp-ingestion": {
        "inputDefinitions": {
          "parameters": {
            "input_location": {
              "type": "STRING"
            }
          }
        },
        "executorLabel": "exec-ingestion",
        "outputDefinitions": {
          "artifacts": {
            "examples": {
              "artifactType": {
                "instanceSchema": "title: kfp.Dataset\ntype: object\nproperties:\n  payload_format:\n    type: string\n  container_format:\n    type: string\n"
              }
            }
          }
        }
      },
      "comp-trainer": {
        "outputDefinitions": {
          "artifacts": {
            "model": {
              "artifactType": {
                "instanceSchema": "title: kfp.Model\ntype: object\nproperties:\n  framework:\n    type: string\n  framework_version:\n    type: string\n"
              }
            }
          }
        },
        "executorLabel": "exec-trainer",
        "inputDefinitions": {
          "artifacts": {
            "examples": {
              "artifactType": {
                "instanceSchema": "title: kfp.Dataset\ntype: object\nproperties:\n  payload_format:\n    type: string\n  container_format:\n    type: string\n"
              }
            }
          },
          "parameters": {
            "n_epochs": {
              "type": "INT"
            },
            "optimizer": {
              "type": "STRING"
            }
          }
        }
      }
    },
    "root": {
      "dag": {
        "tasks": {
          "task-ingestion": {
            "componentRef": {
              "name": "comp-ingestion"
            },
            "inputs": {
              "parameters": {
                "input_location": {
                  "componentInputParameter": "input_location"
                }
              }
            },
            "taskInfo": {
              "name": "task-ingestion"
            }
          },
          "task-trainer": {
            "dependentTasks": [
              "task-ingestion"
            ],
            "taskInfo": {
              "name": "task-trainer"
            },
            "componentRef": {
              "name": "comp-trainer"
            },
            "inputs": {
              "artifacts": {
                "examples": {
                  "taskOutputArtifact": {
                    "producerTask": "task-ingestion",
                    "outputArtifactKey": "examples"
                  }
                }
              },
              "parameters": {
                "optimizer": {
                  "componentInputParameter": "optimizer"
                },
                "n_epochs": {
                  "componentInputParameter": "n_epochs"
                }
              }
            }
          }
        }
      },
      "inputDefinitions": {
        "parameters": {
          "n_epochs": {
            "type": "INT"
          },
          "optimizer": {
            "type": "STRING"
          }
        },
        "artifacts": {
          "input_location": {
            "artifactType": {
              "instanceSchema": "title: kfp.Artifact\ntype: object\n"
            }
          }
        }
      }
    }
  },
  "runtimeConfig": {
    "parameters": {
      "input_location": {
        "stringValue": "gs://test-bucket/pipeline_root"
      },
      "optimizer": {
        "stringValue": "sgd"
      },
      "n_epochs": {
        "stringValue": "200"
      }
    },
    "gcsOutputDirectory": "dummy_root"
  }
}
