      },
      "comp-flip-coin": {
        "outputDefinitions": {
          "parameters": {
            "Output": {
              "type": "STRING"
            }
          }
        },
        "executorLabel": "exec-flip-coin"
      },
      "comp-condition-1": {
        "dag": {
          "tasks": {
            "task-print-msg-2": {
              "dependentTasks": [
                "task-flip-coin-2"
              ],
              "taskInfo": {
                "name": "task-print-msg-2"
              },
              "componentRef": {
                "name": "comp-print-msg"
              },
              "inputs": {
                "parameters": {
                  "msg": {
                    "taskOutputParameter": {
                      "producerTask": "task-flip-coin-2",
                      "outputParameterKey": "Output"
                    }
                  }
                }
              }
            },
            "task-flip-coin-2": {
              "taskInfo": {
                "name": "task-flip-coin-2"
              },
              "componentRef": {
                "name": "comp-flip-coin"
              }
            }
          }
        },
        "inputDefinitions": {
          "parameters": {
            "flip-coin-Output": {
              "type": "STRING"
            }
          }
        }
      }
