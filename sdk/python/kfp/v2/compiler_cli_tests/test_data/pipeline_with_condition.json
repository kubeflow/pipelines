            "inputs": {
              "parameters": {
                "flip-coin-Output": {
                  "taskOutputParameter": {
                    "producerTask": "task-flip-coin",
                    "outputParameterKey": "Output"
                  }
                }
              }
            },
            "componentRef": {
              "name": "comp-condition-1"
            },
            "taskInfo": {
