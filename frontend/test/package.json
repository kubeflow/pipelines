{
  "name": "pipelines-frontend-test",
  "version": "0.0.1",
  "description": "Test package for ML Pipeline Management frontend",
  "dependencies": {
    "@types/assert": "0.0.31",
    "@types/jasmine": "^2.8.6",
    "@types/sinon": "^4.3.0",
    "jasmine": "^2.9.0",
    "jasmine-ts": "^0.2.1",
    "sinon": "^2.0.0-pre",
    "start-server-and-test": "^1.4.1",
    "ts-node": "^5.0.1",
    "typescript": "^2.7.2",
    "wdio-dot-reporter": "0.0.9",
    "wdio-jasmine-framework": "^0.3.2",
    "wdio-selenium-standalone-service": "0.0.10",
    "wdio-visual-regression-service": "^0.8.0",
    "wdio-webcomponents": "^1.1.0",
    "webdrivercss": "^1.1.10",
    "webdriverio": "^4.12.0"
  },
  "scripts": {
    "docker": "docker build -t ml-pipelines-frontend-tests .",
    "start": "cd .. && npm start",
    "component-tests": "wdio components/wdio.conf.js",
    "ci-component-tests": "start-server-and-test start http-get://localhost:3000/apis/v1alpha1/healthz component-tests",
    "ui-tests": "wdio ui/wdio.conf.js",
    "unit-tests": "jasmine-ts unit/*.ts",
    "test": "npm run unit-tests & HEADLESS_UI_TESTS=1 npm run ui-tests",
    "ci": "start-server-and-test start-server http://localhost:3000 ui-tests"
  }
}
