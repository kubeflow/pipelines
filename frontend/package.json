{
  "name": "pipelines-frontend",
  "version": "0.0.7",
  "description": "Frontend package for ML Pipeline Management",
  "main": "dist/index.html",
  "dependencies": {
    "d3": "^4.13.0",
    "dagre": "^0.8.2",
    "js-yaml": "^3.11.0"
  },
  "devDependencies": {
    "@types/d3": "^4.12.0",
    "@types/dagre": "^0.7.39",
    "@types/express": "^4.11.1",
    "@types/js-yaml": "^3.10.1",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.2",
    "babel-minify-webpack-plugin": "^0.3.0",
    "bower": "^1.8.2",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.17.4",
    "mocha": "^5.0.5",
    "polymer-webpack-loader": "^2.0.0",
    "reflect-metadata": "^0.1.10",
    "ts-loader": "^2.3.7",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^5.1.0",
    "tslint-loader": "^3.5.3",
    "typescript": "2.6.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.2"
  },
  "scripts": {
    "mock:api": "ts-node mock-backend/mock-api-server.ts 3001",
    "mock:server": "ML_PIPELINE_SERVICE_HOST=localhost ML_PIPELINE_SERVICE_PORT=3001 node server/dist/server.js dist",
    "build": "webpack --config webpack.prod.ts && cd ./server && npm run build",
    "docker": "docker build -q -t ml-pipelines-frontend:`node -p \"require('./package.json').version\"` .",
    "dev": "webpack-dev-server --config webpack.dev.ts",
    "lint": "tslint -p tsconfig.json",
    "postinstall": "bower install && cd ./server && npm install",
    "start": "npm run dev",
    "tsc": "tsc"
  },
