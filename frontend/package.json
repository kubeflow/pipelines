  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/d3": "^4.12.0",
    "@types/dagre": "^0.7.39",
    "@types/express": "^4.11.1",
    "@types/js-yaml": "^3.10.1",
    "@types/node": "^9.6.2",
    "@types/sinon": "^4.3.0",
    "@types/webpack": "^4.4.8",
    "babel-minify-webpack-plugin": "^0.3.0",
