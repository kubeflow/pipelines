  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/clean-webpack-plugin": "^0.1.2",
    "@types/codemirror": "0.0.60",
    "@types/copy-webpack-plugin": "^4.4.2",
    "@types/d3": "^5.0.0",
    "@types/dagre": "^0.7.39",
    "@types/express": "^4.11.1",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/js-yaml": "^3.10.1",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/node": "^10.7.1",
    "@types/sinon": "^5.0.1",
    "@types/split.js": "^1.3.1",
    "@types/webpack": "^4.4.8",
    "@types/webpack-dev-server": "^2.9.6",
    "@types/webpack-merge": "^4.1.3",
    "babel-minify-webpack-plugin": "^0.3.0",
    "bower": "^1.8.2",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.18.0",
    "mini-css-extract-plugin": "^0.4.1",
    "polymer-webpack-loader": "^2.0.0",
    "reflect-metadata": "^0.1.10",
    "sinon": "^6.1.5",
    "svg-url-loader": "^2.3.2",
    "swagger-ts-client": "^0.9.6",
    "ts-loader": "4.4.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4"
  },
  "scripts": {
    "mock:api": "ts-node mock-backend/mock-api-server.ts 3001",
    "mock:server": "ML_PIPELINE_SERVICE_HOST=localhost ML_PIPELINE_SERVICE_PORT=3001 node server/dist/server.js dist",
    "build": "npm run apis && webpack --config webpack.prod.ts && cd ./server && npm run build",
    "docker": "COMMIT_HASH=`git rev-parse HEAD`; docker build -q -t ml-pipelines-frontend:${COMMIT_HASH} --build-arg COMMIT_HASH=${COMMIT_HASH} --build-arg DATE=\"`date -u`\" -f Dockerfile ..",
    "dev": "webpack-dev-server --config webpack.dev.ts",
    "lint": "tslint -p tsconfig.json",
    "postinstall": "bower install && cd ./server && npm install && cd ../test && npm install",
    "apis:pipeline": "swagger-ts-client -s ../backend/api/swagger/pipeline.swagger.json -t ./src/api/pipeline.ts -o ./src/api",
    "apis:job": "swagger-ts-client -s ../backend/api/swagger/job.swagger.json -t ./src/api/job.ts -o ./src/api",
    "apis:run": "swagger-ts-client -s ../backend/api/swagger/run.swagger.json -t ./src/api/run.ts -o ./src/api",
    "apis": "npm run apis:pipeline && npm run apis:job && npm run apis:run",
    "start": "npm run apis && npm run dev",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/googleprivate/ml.git"
  },
  "author": "",
  "license": "",
  "bugs": {
    "url": "https://github.com/googleprivate/ml/issues"
  },
  "homepage": "https://github.com/googleprivate/ml#readme"
}
