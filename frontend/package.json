{
  "name": "pipelines-frontend",
  "description": "Frontend package for ML Pipeline Management",
  "main": "dist/index.html",
  "dependencies": {
    "codemirror": "^5.40.2",
    "d3": "^5.5.0",
    "dagre": "^0.8.2",
