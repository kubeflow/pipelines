{
  "name": "pipelines-frontend-webserver",
  "version": "0.0.1",
  "description": "Frontend webserver package for ML Pipeline Management",
  "main": "server.js",
  "dependencies": {
    "@google-cloud/storage": "^1.6.0",
    "@kubernetes/client-node": "^0.3.0",
    "express": "^4.16.3",
    "http-proxy-middleware": "^0.18.0",
    "tmp": "0.0.33"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/google-cloud__storage": "^1.1.7",
    "@types/tmp": "0.0.33",
    "typescript": "2.6.2"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "npm run writeversion & npm run writegithash & npm run writebuilddate",
    "writeversion": "node -p \"require('../package.json').version\" > ./dist/VERSION",
    "writegithash": "git rev-parse HEAD > ./dist/COMMIT_HASH",
    "writebuilddate": "date > ./dist/BUILD_DATE"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/googleprivate/ml.git"
  },
  "author": "",
  "license": "",
  "bugs": {
    "url": "https://github.com/googleprivate/ml/issues"
  },
  "homepage": "https://github.com/googleprivate/ml#readme"
}
