{
   "components":{
      "comp-dataflow-python":{
         "executorLabel":"exec-dataflow-python",
         "inputDefinitions":{
            "parameters":{
               "args":{
                  "parameterType":"LIST"
               },
               "location":{
                  "parameterType":"STRING"
               },
               "project":{
                  "parameterType":"STRING"
               },
               "python_module_path":{
                  "parameterType":"STRING"
               },
               "requirements_file_path":{
                  "parameterType":"STRING"
               },
               "temp_location":{
                  "parameterType":"STRING"
               }
            }
         },
         "outputDefinitions":{
            "parameters":{
               "gcp_resources":{
                  "parameterType":"STRING"
               }
            }
         }
      }
   },
   "deploymentSpec":{
      "executors":{
         "exec-dataflow-python":{
            "container":{
               "args":[
                  "--project",
                  "{{$.inputs.parameters['project']}}",
                  "--location",
                  "{{$.inputs.parameters['location']}}",
                  "--python_module_path",
                  "{{$.inputs.parameters['python_module_path']}}",
                  "--temp_location",
                  "{{$.inputs.parameters['temp_location']}}",
                  "--requirements_file_path",
                  "{{$.inputs.parameters['requirements_file_path']}}",
                  "--args",
                  "{{$.inputs.parameters['args']}}",
                  "--gcp_resources",
                  "{{$.outputs.parameters['gcp_resources'].output_file}}"
               ],
               "command":[
                  "python3",
                  "-u",
                  "-m",
                  "google_cloud_pipeline_components.container.experimental.dataflow.dataflow_launcher"
               ],
               "image":"gcr.io/ml-pipeline/google-cloud-pipeline-components:latest"
            }
         }
      }
   },
   "pipelineInfo":{
      "name":"dataflow-python-test"
   },
   "root":{
      "dag":{
         "tasks":{
            "dataflow-python":{
               "cachingOptions":{
                  "enableCache":true
               },
               "componentRef":{
                  "name":"comp-dataflow-python"
               },
               "inputs":{
                  "parameters":{
                     "args":{
                        "runtimeValue":{
                           "constant":[
                              "test_arg"
                           ]
                        }
                     },
                     "location":{
                        "runtimeValue":{
                           "constant":"us-central1"
                        }
                     },
                     "project":{
                        "runtimeValue":{
                           "constant":"test_project"
                        }
                     },
                     "python_module_path":{
                        "runtimeValue":{
                           "constant":"gs://python_module_path"
                        }
                     },
                     "requirements_file_path":{
                        "runtimeValue":{
                           "constant":"gs://requirements_file_path"
                        }
                     },
                     "temp_location":{
                        "runtimeValue":{
                           "constant":"gs://temp_location"
                        }
                     }
                  }
               },
               "taskInfo":{
                  "name":"dataflow-python"
               }
            }
         }
      }
   }
}
