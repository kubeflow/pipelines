{
  "pipelineSpec": {
    "components": {
      "comp-execute-notebook": {
        "executorLabel": "exec-execute-notebook",
        "inputDefinitions": {
          "parameters": {
            "accelerator_core_count": {
              "type": "STRING"
            },
            "block_pipeline": {
              "type": "STRING"
            },
            "container_image_uri": {
              "type": "STRING"
            },
            "execution_id": {
              "type": "STRING"
            },
            "fail_pipeline": {
              "type": "STRING"
            },
            "input_notebook_file": {
              "type": "STRING"
            },
            "labels": {
              "type": "STRING"
            },
            "location": {
              "type": "STRING"
            },
            "master_type": {
              "type": "STRING"
            },
            "output_notebook_folder": {
              "type": "STRING"
            },
            "parameters": {
              "type": "STRING"
            },
            "project": {
              "type": "STRING"
            }
          }
        },
        "outputDefinitions": {
          "parameters": {
            "error": {
              "type": "STRING"
            },
            "gcp_resources": {
              "type": "STRING"
            },
            "output_notebook_file": {
              "type": "STRING"
            },
            "state": {
              "type": "STRING"
            }
          }
        }
      }
    },
    "deploymentSpec": {
      "executors": {
        "exec-execute-notebook": {
          "container": {
            "args": [
              "--init.project",
              "{{$.inputs.parameters['project']}}",
              "--init.input_notebook_file",
              "{{$.inputs.parameters['input_notebook_file']}}",
              "--init.output_notebook_folder",
              "{{$.inputs.parameters['output_notebook_folder']}}",
              "--init.execution_id",
              "{{$.inputs.parameters['execution_id']}}",
              "--method.location",
              "{{$.inputs.parameters['location']}}",
              "--method.master_type",
              "{{$.inputs.parameters['master_type']}}",
              "--method.accelerator_core_count",
              "{{$.inputs.parameters['accelerator_core_count']}}",
              "--method.labels",
              "{{$.inputs.parameters['labels']}}",
              "--method.container_image_uri",
              "{{$.inputs.parameters['container_image_uri']}}",
              "--parameters",
              "{{$.inputs.parameters['parameters']}}",
              "--method.block_pipeline",
              "{{$.inputs.parameters['block_pipeline']}}",
              "--method.fail_pipeline",
              "{{$.inputs.parameters['fail_pipeline']}}",
              "----output-paths",
              "{{$.outputs.parameters['state'].output_file}}",
              "{{$.outputs.parameters['output_notebook_file'].output_file}}",
              "{{$.outputs.parameters['gcp_resources'].output_file}}",
              "{{$.outputs.parameters['error'].output_file}}"
            ],
            "command": [
              "python3",
              "-m",
              "google_cloud_pipeline_components.container.experimental.notebooks.executor"
            ],
            "image": "gcr.io/mam-nooage/google-cloud-pipeline-components:latest"
          }
        }
      }
    },
    "pipelineInfo": {
      "name": "notebook-as-a-step-sample"
    },
    "root": {
      "dag": {
        "tasks": {
          "execute-notebook": {
            "cachingOptions": {
              "enableCache": true
            },
            "componentRef": {
              "name": "comp-execute-notebook"
            },
            "inputs": {
              "parameters": {
                "accelerator_core_count": {
                  "runtimeValue": {
                    "constantValue": {
                      "stringValue": "0"
                    }
                  }
                },
                "block_pipeline": {
                  "runtimeValue": {
                    "constantValue": {
                      "stringValue": "True"
                    }
                  }
                },
                "container_image_uri": {
                  "runtimeValue": {
                    "constantValue": {
                      "stringValue": "gcr.io/deeplearning-platform-release/base-cpu:latest"
                    }
                  }
                },
                "execution_id": {
                  "componentInputParameter": "execution_id"
                },
                "fail_pipeline": {
                  "runtimeValue": {
                    "constantValue": {
                      "stringValue": "True"
                    }
                  }
                },
                "input_notebook_file": {
                  "componentInputParameter": "input_notebook"
                },
                "labels": {
                  "runtimeValue": {
                    "constantValue": {
                      "stringValue": "src=notebooks_executor_api"
                    }
                  }
                },
                "location": {
                  "runtimeValue": {
                    "constantValue": {
                      "stringValue": "us-central1"
                    }
                  }
                },
                "master_type": {
                  "runtimeValue": {
                    "constantValue": {
                      "stringValue": "n1-standard-4"
                    }
                  }
                },
                "output_notebook_folder": {
                  "componentInputParameter": "output_notebook_folder"
                },
                "parameters": {
                  "runtimeValue": {
                    "constantValue": {
                      "stringValue": "PROJECT_ID={{$.inputs.parameters['pipelineparam--project_id']}},EXECUTION_ID={{$.inputs.parameters['pipelineparam--execution_id']}}"
                    }
                  }
                },
                "pipelineparam--execution_id": {
                  "componentInputParameter": "execution_id"
                },
                "pipelineparam--project_id": {
                  "componentInputParameter": "project_id"
                },
                "project": {
                  "componentInputParameter": "project_id"
                }
              }
            },
            "taskInfo": {
              "name": "execute-notebook"
            }
          }
        }
      },
      "inputDefinitions": {
        "parameters": {
          "execution_id": {
            "type": "STRING"
          },
          "input_notebook": {
            "type": "STRING"
          },
          "output_notebook_folder": {
            "type": "STRING"
          },
          "project_id": {
            "type": "STRING"
          }
        }
      }
    },
    "schemaVersion": "2.0.0",
    "sdkVersion": "kfp-1.8.2"
  },
  "runtimeConfig": {
    "gcsOutputDirectory": "gs://mam-nooage-vertex-root"
  }
}